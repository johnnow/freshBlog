<% include header.ejs %>

<div class="container">
    <div class="row">
        <div class="col-md-10">
            <ol class="breadcrumb">
                <li class="active">由于还没有在线模拟器，因此转向jsFiddle展示源码TAT</li>
            </ol>
        </div>
    </div>
</div>

<div class="container">
    <h1>不可方物</h1>
    <div class="row">
        <div class="col-md-offset-1 col-md-10">
            <% mocks.forEach(function(mock,index){ %>
            <div class="col-xs-6 col-md-3">
                <a target="_blank" href="<%=mock.url%>" class="sharpThumb">
                    <img style="width:100%;height:100%;" src="/public/images/<%=mock.albumImg%>" alt="仿物志封面照"/>
                </a>
                <div class="caption">
                    <h3><%=mock.title%></h3>
                </div>
            </div>
            <% }); %>
        </div>
    </div>
</div>


<% include footer.ejs %>